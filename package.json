  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "react-native": ">=0.70.0"
  },
  "devDependencies": {
    "typescript": "^5.8.0",
    "vitest": "^2.0.0"
  },
  "react-native": "./dist/index.js",
  "source": "./src/index.ts"
}
